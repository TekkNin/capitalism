@model Capitalism.Logic.Models.Buildings.Forest

<h4>The Forest</h4>

<div class="mt-3">
    <div class="tab-content border p-3">
        <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
            <p>Chopping wood in the forest gives you basic contruction materials so that you can build your first structures.</p>
            <hr />
            <div class="w-100">
                <button class="btn btn-dark" onclick="work('@Model.Id')">Chop Wood</button>
            </div>
        </div>
    </div>
</div>