@model PlayerCreateViewModel
@{
    ViewData["Title"] = "Create New Player";
}

<h1>Create New Player</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="DisplayName" class="col-form-label"></label>
        <input asp-for="DisplayName" type="text" class="form-control" autocomplete="off" />
        <span asp-validation-for="DisplayName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <button type="submit" value="Submit">Create Player</button>
    </div>
</form>